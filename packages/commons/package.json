{
  "name": "@magician/core",
  "version": "0.1.0",
  "description": "The common modules for Dark Magician",
  "types": "@types/index.d.ts",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "pretest": "export TS_NODE_FILES=true",
    "test": "nyc --reporter lcov ava",
    "lint": "eslint --ext=ts lib tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whoastonic/DarkMagician.git"
  },
  "keywords": [
    "discord",
    "discord-bot",
    "bot",
    "node",
    "typescript"
  ],
  "author": "whoastonic <kingstonicthe1@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/whoastonic/DarkMagician/issues"
  },
  "homepage": "https://github.com/whoastonic/DarkMagician#readme",
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/node": "^16.3.3",
    "dotenv": "^10.0.0"
  },
  "dependencies": {
    "@mikro-orm/reflection": "^4.5.7",
    "docker-secret-env": "^0.2.1",
    "winston": "^3.3.3",
    "yaml": "^1.10.2"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "!lib",
      "!dist",
      "!__tests__/data"
    ],
    "require": [
      "tsconfig-paths/register",
      "ts-node/register"
    ]
  }
}
