{
  "name": "@dmg/cache",
  "version": "1.0.1-alpha.0",
  "description": "A simple overrall cache for DarkMagician",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "test": "nyc --reporter lcov ava"
  },
  "author": "whoastonic <kingstonicthe1@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "ioredis": "^4.27.11"
  },
  "devDependencies": {
    "@types/ioredis": "^4.27.6"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "!lib",
      "!dist",
      "!__tests__/data"
    ],
    "require": [
      "tsconfig-paths/register",
      "ts-node/register"
    ]
  }
}
