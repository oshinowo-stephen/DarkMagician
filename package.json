{
  "name": "magician",
  "version": "1.0.0",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "description": "Another YGO Discord Bot.",
  "scripts": {
    "postinstall": "lerna bootstrap",
    "lint": "lerna run lint --no-bail",
    "test": "lerna run test --no-bail",
    "build": "lerna run build --no-bail",
    "reinstall": "npm run clean && npm i",
    "clean": "lerna clean --yes && rm -rf ./node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whoastonic/DarkMagician.git"
  },
  "author": "whoastonic <kingstonicthe1@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/whoastonic/DarkMagician/issues"
  },
  "homepage": "https://github.com/whoastonic/DarkMagician#readme",
  "devDependencies": {
    "@alex-taxiera/eslint-config-ts": "^1.3.2",
    "@ovos-media/ts-transform-paths": "^1.7.18-1",
    "@types/node": "^16.10.3",
    "@types/sinon": "^10.0.2",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "ava": "^4.3.0",
    "eslint": "^7.31.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-modules-newline": "0.0.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "lerna": "^4.0.0",
    "nyc": "^15.1.0",
    "sinon": "^11.1.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "!lib",
      "!dist",
      "!__tests__/data"
    ],
    "require": [
      "tsconfig-paths/register",
      "ts-node/register"
    ]
  }
}
