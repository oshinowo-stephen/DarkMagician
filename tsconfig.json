{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "declaration": true,
    "esModuleInterop": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "alwaysStrict": true,
    "strictNullChecks": false,
  },
  "exclude": [
    "node_modules"
  ]
}
